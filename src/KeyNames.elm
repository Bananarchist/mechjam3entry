    Digit4 -> "Digit4"
    Digit5 -> "Digit5"
    Digit6 -> "Digit6"
    Digit7 -> "Digit7"
    Digit8 -> "Digit8"
    Digit9 -> "Digit9"
    Digit0 -> "Digit0"
    Minus -> "Minus"
    Equal -> "Equal"
    Backspace -> "Backspace"
    Tab -> "Tab"
    Q -> "Q"
    W -> "W"
    E -> "E"
    R -> "R"
