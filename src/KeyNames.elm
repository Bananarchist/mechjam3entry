module KeyNames exposing (keyNames)

import Key exposing (Key(..))

keyNames : Key -> String
keyNames k =
  case k of
    Escape -> "Escape"
    F1 -> "F1"
    F2 -> "F2"
    F3 -> "F3"
    F4 -> "F4"
    F5 -> "F5"
    F6 -> "F6"
